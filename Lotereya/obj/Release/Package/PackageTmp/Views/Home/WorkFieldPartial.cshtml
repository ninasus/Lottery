@model Tuple<int, int>

<div id="game">
    <div class="container">
        <div class="row">
            <div class="col-xs-5">
				<p>Выберите 6 из 42</p>
                <div class="play_field">
                    <div class="grid clearfix">
                        @for (int i = Model.Item1; i <= Model.Item2; i++)
                        {
                            <div class="element" data-id="@i">
                                @i
                            </div>
                        }
                    </div>
                    <div class="btns">
                        <button id="autoSelect" class="btn btn-default active">Авто выбор</button>
                        <button id="reset" class="btn btn-default" disabled>Сброс</button>
                        <button id="play" class="btn btn-default" disabled>Играть</button>
                    </div>
                </div>
            </div>
            <div class="col-xs-7">
                <div class="post">
                     @{Html.RenderAction("OptionPartial", "Home", new { id = 3 });}
                </div>
                <div class="jackpot">
                    ДЖЕКПОТ
                    <span class="sum">0 $</span>
                </div>
            </div>
        </div>
    </div>
</div>
