@model int

<div id="priceElements" class="result">
    <p>Выигрышные номера</p>
    <div class="numbs clearfix">
        @for (int i = 0; i < Model; i++)
        {
            <span class="elementSel"> </span>
        }

    </div>
    <p class="time">До начала розыгрыша осталось <b class="time_to_draw">0</b> сек.</p>
</div>
